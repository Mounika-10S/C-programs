#include<stdio.h>
int subtractProductAndSum(int n){
    int dig,sum,product,result;
    scanf("%d",&n);
    sum=0;
    product=1;
     
    while(1<=n<=10^5)
    {
        dig=n%10; 
        sum+=dig;
        product*=dig;
        n=n/10;
        result=product-sum;
    }
    printf("%d\n",result);   
    return 0;
}
